{
  "name": "evoting-platform",
  "version": "2.1.0",
  "description": "Plateforme de vote en ligne sécurisée et fiable avec notifications temps réel",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "init-db": "node server/scripts/init-db-supabase.js",
    "migrate": "node server/scripts/migrate.js",
    "migrate:v2": "node server/scripts/migrate-v2.js",
    "migrate:notifications": "node server/scripts/migrate-notifications.js",
    "generate-keys": "node server/scripts/generate-keys.js",
    "generate-vapid": "node server/scripts/generate-vapid.js",
    "test:supabase": "node server/scripts/test-supabase.js",
    "test:db": "node server/scripts/test-supabase.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "voting",
    "election",
    "democracy",
    "secure-voting"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csv-parse": "^5.5.3",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-code": "^2.0.12",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.10.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "jsdom": "^27.0.0",
    "nodemon": "^3.0.2",
    "terser": "^5.44.0",
    "vite": "^5.0.8",
    "vitest": "^3.2.4"
  }
}
